{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es2015",
    "module": "ES2020",
    "lib": ["es2020","dom"],
    "removeComments": false,
    "outDir": "./compiledJs",
    "sourceMap": true,
    "allowJs": true
  },
  "include": [
    "./ts/**/*"
  ],
  "exclude": [
    "node_modules",
    "../**/*"
  ]
}